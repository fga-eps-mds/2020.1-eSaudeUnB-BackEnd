        "commonjs": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["error", 4],
        "arrow-parens": ["error", "always"]
    }
}
